<div class="card">
    <div class="card-body">
        <div class="table-responsive">
            <table class="table card-table table-vcenter text-nowrap">
                <thead>
                    <tr>
                        <th>
                            Manga Name
                        </th>
                        <th>
                            Genres
                        </th>
                        <th>
                            Hits
                        </th>
                    </tr>
                </thead>
                <tbody>
                    <% list.forEach(function(manga){ %>
                        <tr>
                            <td>
                                <a href="/#/manga/<%=manga.i %>"><%=manga.t %></a>
                            </td>
                            <td style="width: 100px;">
                                <%=manga.c.join(', ') %>
                            </td>
                            <td>
                                <%=manga.h %>
                            </td>
                        </tr>
                    <% }); %>
                </tbody>
            </table>
        </div>
    </div>
</div>
<div class="card">
    <div class="card-header">
        <nav aria-label="Page navigation example">
            <ul class="pagination">
                <% for(var i = 0; i < pagecount; i++)  { %>
                    <li class="page-item">
                        <a class="page-link" href="all-manga?page=<%= i+1 %>"><%= i+1 %></a>
                    </li>
                <% } %>
            </ul>
        </nav>
    </div>
</div>